<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esatic.events">

    <uses-permission
        android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission
        android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/event_launcher_2_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/event_launcher_2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Events">
        <activity android:name=".gps"></activity>
        <activity android:name=".ListEvent" />
        <activity android:name=".AddingEvent" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>